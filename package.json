{
	"name": "@gabortorma/release-it-buildver-plugin",
	"version": "1.0.0",
	"description": "Build Versioning plugin for Release It!",
	"main": "index.js",
	"scripts": {
		"release": "release-it"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gabortorma/release-it-buildver-plugin.git"
	},
	"keywords": [
		"release",
		"release-it",
		"release-it-plugin",
		"version",
		"bump",
		"manifest",
		"buildver"
	],
	"author": {
		"name": "GÃ¡bor Torma",
		"email": "gabor@torma.dev"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/gabortorma/release-it-buildver-plugin/issues"
	},
	"homepage": "https://github.com/gabortorma/release-it-buildver-plugin#readme",
	"type": "module",
	"devDependencies": {
		"release-it": "^16.2.1"
	},
	"peerDependencies": {
		"release-it": "^16.1.3"
	},
	"release-it": {
		"git": {
			"requireCleanWorkingDir": false
		},
		"github": {
			"release": true
		},
		"npm": {
			"publish": true
		},
		"plugins": {
			"./index.js": {
				"format": "yyyy.mm.minor"
			}
		}
	}
}
